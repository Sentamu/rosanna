<%= form_for(@kisting, html: { multipart: true }) do |f| %>
  <% if @kisting.errors.any? %>
    <div id="error_explanation" class="alert alert-danger alert-dismissable">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <h4><%= pluralize(@kisting.errors.count, "error") %> prohibited this listing from being saved:</h4>

      <ul>
      <% @kisting.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :description %><br>
    <%= f.text_area :description, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :sellprice %><br>
    <%= f.text_field :sellprice, class: "form-control" %>
  </div>
 <div class="form-group">
    <%= f.label :rentprice %><br>
    <%= f.text_field :rentprice, class: "form-control" %>
  </div>
   <div class="form-group">
    <%= f.label :Image %><br>
    <%= f.file_field :image, class: "form-control" %>
  </div>
 <div class="form-group">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>